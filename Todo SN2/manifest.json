{


    "manifest_version": 2,
    "name": "Todo SN2",
    "description": "todo list test 2",
    "version": "1.0",
    "content_scripts": [{
        "matches": [
            "*://*/*"
        ],
        "js": [
            "todo_popup.js"
        ],
        "run_at": "document_end"
    }],
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_popup": "todo_popup.html"
    }
}